{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/user/(.*)",
      "dest": "backend/index.js"
    },
    {
      "src": "/api/v1/notes/(.*)",
      "dest": "backend/index.js"
    },
    {
      "src": "/api/v1/user/register",
      "dest": "backend/index.js"
    },
    {
      "src": "/api/v1/user/login",
      "dest": "backend/index.js"
    },
    {
      "src": "/api/v1/user/logout",
      "dest": "backend/index.js"
    },
    {
      "src": "/api/v1/user/profile/update",
      "dest": "backend/index.js"
    }
  ]
}
